.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #b3d4bd;
}
.user-avter {
    margin-top: 162rpx;
    width: 200rpx;
    height: 200rpx;
    border-radius: 100rpx;
}
.user-name {
    font-weight: bold;
    font-size: 32rpx;
}
.moto-contenter {
    margin-top: 200rpx;
    border: 1px solid #405f80;
    height: 80rpx;
    width: 200rpx;
    border-radius: 5rpx;
    text-align: center;
}
.moto {
    background: #405f80;
    color: white;
    height: 100%;
    width: 100%;
    line-height: 80rpx;
    font-weight: bold;
    font-size: 22rpx;
}
